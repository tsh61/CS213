<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.SListController">
	
	<ListView fx:id="listView" prefHeight="275.0" prefWidth="248.0" AnchorPane.bottomAnchor="175.0" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10.0" />
   <Button fx:id="Add" layoutX="14.0" layoutY="285.0" mnemonicParsing="false" onAction="#addSong" text="Add" />
   <Button fx:id="Edit" layoutX="115.0" layoutY="285.0" mnemonicParsing="false" onAction="#editSong" text="Edit" />
   <Button fx:id="Delete" layoutX="200.0" layoutY="285.0" mnemonicParsing="false" onAction="#deleteSong" text="Delete" />
   <TextField fx:id="Title" layoutX="97.0" layoutY="321.0" promptText="Required" />
   <TextField fx:id="Artist" layoutX="97.0" layoutY="348.0" promptText="Required" />
   <TextField fx:id="Album" layoutX="97.0" layoutY="375.0" />
   <TextField fx:id="Year" layoutX="97.0" layoutY="402.0" />
   <Label layoutX="40.0" layoutY="326.0" text="Title :" />
   <Label layoutX="40.0" layoutY="353.0" text="Artist :" />
   <Label layoutX="40.0" layoutY="380.0" text="Album :" />
   <Label layoutX="40.0" layoutY="407.0" text="Year :" />
	
</AnchorPane>
